allowHTML: true
langs: ['ru', 'en']

ignore:
  - "en/_api-ref/*"
  - "en/_api-ref-grpc/*"
  - "ru/_api-ref/*"
  - "ru/_api-ref-grpc/*"
  - "ru/tracker/*"
  - "ru/monitoring/*"

docs-viewer:
  project-name: cloud
  langs: ['ru', 'en']
  show-revision: false
  show-toc-header: true
  releasers:
    - elenbaskakova
    - achegodaev
  index-page:
    hidden: false
    title: Yandex Cloud
    description: Документация Yandex.Cloud
    service-url: https://yc.yandex-team.ru/
    url:
      testing: https://docs.yandex-team.ru/cloud/
      production: https://docs.yandex-team.ru/cloud/

apply-presets: true
varsPreset: 'internal'