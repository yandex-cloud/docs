docs-viewer:
  project-name: cloud
  abc-slug: cloud
  langs: ['ru', 'en']
  show-revision: false
  show-toc-header: true
  vcs-url-prefix: https://github.com/yandex-cloud/docs-source/tree/develop
  startrek:
    queue: YCDOCS
    ticket-type: bug
  releasers:
    - elenbaskakova
    - achegodaev
  index-page:
    hidden: false
    title: Yandex Cloud
    description: Документация Yandex Cloud
    service-url: https://yc.yandex-team.ru/
  search-by-toc: true

  auto-release-to: # Авторелизы
    testing: true
    prod: true

apply-presets: true
