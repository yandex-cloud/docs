allowHTML: true
langs: ['ru']

ignore:
  - "en/_api-ref/*"
  - "en/_api-ref-grpc/*"
  - "ru/_api-ref/*"
  - "ru/_api-ref-grpc/*"
  - "ru/tracker/api-ref/*"
  - "en/tracker/api-ref/*"
  - "ru/monitoring/*"
  - "ru/data-streams/*"
  - "en/*"

docs-viewer:
  project-name: cloud
  abc-slug: cloud
  langs: ['ru']
  show-revision: false
  show-toc-header: true
  startrek:
    queue: YCDOCS
    ticket-type: bug
  releasers:
    - elenbaskakova
    - achegodaev
  index-page:
    hidden: false
    title: Yandex Cloud
    description: Документация Yandex.Cloud
    service-url: https://yc.yandex-team.ru/
    url:
      testing: https://docs.yandex-team.ru/cloud/
      production: https://docs.yandex-team.ru/cloud/

apply-presets: true
varsPreset: 'internal'